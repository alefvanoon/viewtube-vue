  "name": "viewtube",
  "description": "ViewTube, an alternative Youtube frontend.",
  "author": "Maurice Oegerli",
  "country": "Switzerland",
  "email": "viewtube@m-o.dev",
  "version": "0.5.0",
  "license": "AGPLv3",
  "private": true,
  "scripts": {
    "start:local": "cross-env NODE_ENV=production VIEWTUBE_API_URL=http://localhost:8066/api/ && node -r module-alias/register dist/server/main.js",
    "start": "node -r module-alias/register dist/server/main.js",
    "build:local": "cross-env NODE_ENV=production npm run build",
    "build": "rimraf dist && nuxt build && tsc -p tsconfig.build.json",
    "serve": "cross-env NODE_ENV=development VIEWTUBE_API_URL=http://localhost:8066/api/ webpack --config webpack.config.js",
    "test": "jest",
    "db": "mongod --dbpath data",
    "prettier": "prettier --write .",
    "lint": "eslint --ext .ts,.js,.vue .",
    "clean": "rimraf dist",
    "nest": "nest",
    "nuxt": "nuxt",
    "postinstall": "husky install",
    "lint-staged": "prettier --write --ignore-unknown"
  },
  "_moduleAliases": {
    "server": "dist/server"
  },
  "browserslist": [
    "last 1 version",
    "not IE 11",
    "not IE_Mob 11",
    "not Edge 18"
